  <body>
  <div class="header"> 
    <%- include('common/header') %>
  </div>
  <!-- Hero Section with Search Box start -->
  <section class="hero-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 mx-auto text-center">
                <h1 class="display-4 fw-bold mb-4">Discover Your Perfect Adventure</h1>
                <p class="lead mb-5">Explore thousands of tours, activities, and getaways around the world</p>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-10 mx-auto">
                <div class="search-box">
                    <form id="tourSearchForm">
                        <div class="row g-3">
                            <div class="col-md-6">
                                <label for="destination" class="form-label">Destination</label>
                                <input type="text" class="form-control" id="destination" placeholder="Where do you want to go?">
                            </div>
                            <div class="col-md-3">
                                <label for="date" class="form-label">Date</label>
                                <input type="date" class="form-control" id="date">
                            </div>
                            <div class="col-md-3">
                                <label for="duration" class="form-label">Duration</label>
                                <select class="form-select" id="duration">
                                    <option selected>Any duration</option>
                                    <option>1-3 days</option>
                                    <option>4-7 days</option>
                                    <option>8-14 days</option>
                                    <option>15+ days</option>
                                </select>
                            </div>
                            <div class="col-12">
                                <button type="submit" class="btn btn-primary w-100 py-3">Search Tours</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Hero Section with Search Box end -->

